	<queryString language="SQL">
		<![CDATA[SELECT INV_NUMDOC,MAE_RIF,MAE_NOMBRE,INV_FECHA,INV_CODVEN,
SUM(INV_MTOIVA) AS IVA,SUM(INV_DESPRO+INV_DESSUB) AS DESCU,
SUM(INV_PRECIO) AS SUBT,
SUM(INV_PRECIO+INV_MTOIVA-INV_DESPRO-INV_DESSUB) AS TOTAL,
EMP_NOMBRE,EMP_RIF 
FROM DNINVENTARIO
INNER JOIN DNMAESTRO ON INV_CODMAE=MAE_CODIGO
INNER JOIN DNEMPRESAS ON INV_EMPRESA=EMP_CODIGO
WHERE INV_EMPRESA=$P{empresa} 
AND INV_FECHA>=$P{fchdesde}
AND INV_FECHA<=$P{fchhasta}
AND INV_CODDOC='POS'
GROUP BY INV_NUMDOC]]>
	</queryString>
