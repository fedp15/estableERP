	<parameter name="orden" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[SELECT DISTINCT numdoc_orig AS inv_numdoc,dninventario.pers_id,
                         IF(nombre IS NULL,razon_social,IF(nombre='',razon_social,nombre)) AS nombre,rif,
                         SUM(inv_total) AS mtoret,
                         emp_codigo,emp_nombre,emp_rif,(SELECT dir_descri FROM dndireccion WHERE dir_pers_id=dnempresas.pers_id LIMIT 1) AS emp_direccion,
                         @tipiva:=inv_tipiva,tiva_valven,inv_control,
                         (SELECT doc_tiptra FROM dndocumentos 
                          WHERE doc_empresa=$P{empresa}
                                AND doc_codigo=$P{tipdoc}) AS doc_tiptra,
                         (SELECT MAX(inv_fchdoc) FROM dninventario 
                          WHERE inv_empresa=$P{empresa}
                                AND inv_numdoc=$P{numdoc} AND inv_coddoc=$P{tipdoc} 
                                AND inv_activo=1 GROUP BY inv_numdoc) AS inv_fecha,
                         (SELECT SUM(inv_mtoiva) AS iva FROM dninventario 
                          WHERE inv_empresa=$P{empresa}
                                AND inv_numdoc=$P{numdoc} 
                                AND inv_coddoc=$P{tipdoc} 
                                AND inv_activo=1 GROUP BY inv_numdoc) AS iva,
                         (SELECT SUM(inv_total) AS total FROM dninventario 
                          WHERE inv_empresa=$P{empresa}
                                AND inv_numdoc=$P{numdoc} 
                                AND inv_coddoc=$P{tipdoc} 
                                AND inv_activo=1 GROUP BY inv_numdoc) AS total,
						 (SELECT SUM(inv_total) FROM dninventario
						  WHERE inv_empresa=$P{empresa}
							    AND inv_numdoc=$P{numdoc}
							    AND inv_coddoc=$P{tipdoc}
							    AND inv_tipiva<>'EX'
                                AND inv_activo=1 GROUP BY inv_numdoc) AS total_iva,
						 (SELECT SUM(inv_total) FROM dninventario
						  WHERE inv_empresa=$P{empresa}
								AND inv_numdoc=$P{numdoc}
								AND inv_coddoc=$P{tipdoc}
								AND inv_tipiva='EX'
 								AND inv_activo=1 GROUP BY inv_numdoc) AS total_exento,
						 (SELECT SUM(inv_base) FROM dninventario
						  WHERE inv_empresa=$P{empresa}
								AND inv_numdoc=$P{numdoc}
								AND inv_coddoc=$P{tipdoc}
								AND inv_tipiva<>'EX'
 								AND inv_activo=1 GROUP BY inv_numdoc) AS base
                         FROM dninventario 
                         LEFT JOIN dnpersonas ON dninventario.pers_id = dnpersonas.pers_id
                         LEFT JOIN dntipiva ON inv_tipiva = tiva_codigo 
                         INNER JOIN dnempresas ON inv_empresa = emp_codigo
                         WHERE inv_coddoc=$P{tipret}
                               AND inv_numdoc=$P{numret} 
                               AND inv_empresa=$P{empresa}
                               AND inv_orden=$P{orden}
                               AND inv_activo=1 
                         GROUP BY inv_numdoc]]>
	</queryString>
